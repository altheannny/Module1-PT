    public static void main(String[] args) {
        System.out.println("My name is Althea Nyna B. Asca√±o.");
        System.out.println("The current date and time is: " + LocalDateTime.now().format(DateTimeFormatter.ofPattern("yyyy-MM-dd HH:mm:ss")));
    }
}
